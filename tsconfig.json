{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"esModuleInterop": true,
		"module": "ESNext",
		"target": "ES2017",
		"allowJs": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": [
			"DOM",
			"ES2017"
		]
	},
	"include": [
		"**/*.ts"
	]
}